{
    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    // ğŸš€ MEGAZORD CODE v6.0 - NASA LEVEL KEYBINDINGS
    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    // INSTRUÃ‡ÃƒO: Copie para seu keybindings.json global
    // (Ctrl+K Ctrl+S > Abrir JSON de Atalhos do Teclado)
    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    
    "version": "2.0.0",
    "keybindings": [
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• HOG - TRADUÃ‡ÃƒO SEMÃ‚NTICA â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+shift+e",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] ENCODE - Preparar para IA",
            "when": "editorTextFocus"
        },
        {
            "key": "ctrl+shift+d",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] RESTORE - Restaurar Original",
            "when": "editorTextFocus"
        },
        {
            "key": "ctrl+alt+s",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] STATS - Estatisticas"
        },
        {
            "key": "ctrl+alt+p",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] PREVIEW - Ver Alteracoes",
            "when": "editorTextFocus"
        },
        {
            "key": "ctrl+alt+z",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] UNDO - Desfazer Ultima"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• HTTP TOOLKIT â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+alt+h",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] HTTP - Request Simples"
        },
        {
            "key": "ctrl+alt+i",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] HTTP - Intercept (Modo AI)"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• GIT SHORTCUTS â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+shift+g ctrl+shift+p",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] GIT PUSH"
        },
        {
            "key": "ctrl+shift+g ctrl+shift+c",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] GIT COMMIT ALL"
        },
        {
            "key": "ctrl+shift+g ctrl+shift+d",
            "command": "workbench.action.tasks.runTask",
            "args": "[HOG] GIT - SAFE DEPLOY"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• NAVIGATION BOOST â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+tab",
            "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
        },
        {
            "key": "ctrl+shift+tab",
            "command": "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
        },
        {
            "key": "alt+1",
            "command": "workbench.action.openEditorAtIndex1"
        },
        {
            "key": "alt+2",
            "command": "workbench.action.openEditorAtIndex2"
        },
        {
            "key": "alt+3",
            "command": "workbench.action.openEditorAtIndex3"
        },
        {
            "key": "alt+4",
            "command": "workbench.action.openEditorAtIndex4"
        },
        {
            "key": "alt+5",
            "command": "workbench.action.openEditorAtIndex5"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• EDITOR POWER MOVES â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+d",
            "command": "editor.action.copyLinesDownAction",
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "key": "ctrl+shift+d",
            "command": "editor.action.deleteLines",
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "key": "alt+up",
            "command": "editor.action.moveLinesUpAction",
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "key": "alt+down",
            "command": "editor.action.moveLinesDownAction",
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "key": "ctrl+j",
            "command": "editor.action.joinLines",
            "when": "editorTextFocus && !editorReadonly"
        },
        {
            "key": "ctrl+shift+k",
            "command": "editor.action.deleteLines",
            "when": "editorTextFocus && !editorReadonly"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• TERMINAL â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+`",
            "command": "workbench.action.terminal.toggleTerminal"
        },
        {
            "key": "ctrl+shift+`",
            "command": "workbench.action.terminal.new"
        },
        {
            "key": "ctrl+shift+c",
            "command": "workbench.action.terminal.copySelection",
            "when": "terminalFocus && terminalTextSelected"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• SIDEBAR â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+b",
            "command": "workbench.action.toggleSidebarVisibility"
        },
        {
            "key": "ctrl+shift+e",
            "command": "workbench.view.explorer",
            "when": "!editorTextFocus"
        },
        {
            "key": "ctrl+shift+f",
            "command": "workbench.view.search"
        },
        {
            "key": "ctrl+shift+g",
            "command": "workbench.view.scm",
            "when": "!editorTextFocus"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• COPILOT POWER â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+i",
            "command": "github.copilot.chat.attachFile",
            "when": "chatIsEnabled"
        },
        {
            "key": "ctrl+shift+i",
            "command": "workbench.action.chat.open"
        },
        {
            "key": "ctrl+enter",
            "command": "github.copilot.acceptPanelSuggestion",
            "when": "github.copilot.panelVisible"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• MULTI-CURSOR MASTERY â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+alt+up",
            "command": "editor.action.insertCursorAbove",
            "when": "editorTextFocus"
        },
        {
            "key": "ctrl+alt+down",
            "command": "editor.action.insertCursorBelow",
            "when": "editorTextFocus"
        },
        {
            "key": "ctrl+shift+l",
            "command": "editor.action.selectHighlights",
            "when": "editorFocus"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• CODE ACTIONS â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+.",
            "command": "editor.action.quickFix",
            "when": "editorHasCodeActionsProvider && editorTextFocus"
        },
        {
            "key": "f2",
            "command": "editor.action.rename",
            "when": "editorHasRenameProvider && editorTextFocus"
        },
        {
            "key": "f12",
            "command": "editor.action.revealDefinition",
            "when": "editorHasDefinitionProvider && editorTextFocus"
        },
        {
            "key": "alt+f12",
            "command": "editor.action.peekDefinition",
            "when": "editorHasDefinitionProvider && editorTextFocus"
        },
        {
            "key": "shift+f12",
            "command": "editor.action.goToReferences",
            "when": "editorHasReferenceProvider && editorTextFocus"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• FORMATTING â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "shift+alt+f",
            "command": "editor.action.formatDocument",
            "when": "editorHasDocumentFormattingProvider && editorTextFocus"
        },
        {
            "key": "ctrl+k ctrl+f",
            "command": "editor.action.formatSelection",
            "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus"
        },
        
        // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• BOOKMARKS (QUICK JUMPS) â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
        {
            "key": "ctrl+f2",
            "command": "bookmarks.toggle",
            "when": "editorTextFocus"
        },
        {
            "key": "f2",
            "command": "bookmarks.jumpToNext",
            "when": "editorTextFocus && !editorHasRenameProvider"
        }
    ]
}
